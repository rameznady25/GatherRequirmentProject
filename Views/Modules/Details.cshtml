@model Gather_Requirement_Project.Models.Module

@{
    ViewData["Title"] = "Details";
}

<div class="card ">
    <div class="card-header">
        <h4> <i class="fas fa-table"></i>&nbsp&nbsp Details Module </h4>
    </div>
    <div class="card-body" style="">
        <div class="row">
            <div class="col-md-10">

            </div>
            <div class="col-md-2">
                <a asp-action="Index" asp-route-programId="@Model.CustomerProgramID">Go to Back</a>
            </div>
        </div>
        <hr />



        <dl class="text-md-left  row">
            <dt class="col-sm-3">
                <span>Module English Name In Single</span>
            </dt>
            <dd class="col-sm-9">
                @Html.DisplayFor(model => model.EnglishNameInSingle)
            </dd>

            <dt class="col-sm-3">
                <span>Module English Name In Plural</span>
            </dt>
            <dd class="col-sm-9">
                @Html.DisplayFor(model => model.EnglishNameInPlural)
            </dd>

            <dt class="col-sm-3">
                <span>Module Arabic Name In Single</span>
            </dt>
            <dd class="col-sm-9">
                @Html.DisplayFor(model => model.ArabicNameInSingle)
            </dd>


            <dt class="col-sm-3">
                <span>Module Arabic Name In Plural</span>
            </dt>
            <dd class="col-sm-9">
                @Html.DisplayFor(model => model.ArabicNameInPlural)
            </dd>


            <dt class="col-sm-3">
                <span>Module URL For Service</span>
            </dt>
            <dd class="col-sm-9">
                @Html.DisplayFor(model => model.URLForService)
            </dd>

        </dl>


        <dl class="col-sm-5">
            <b><a asp-action="Edit" asp-route-id="@Model.ID">Edit</a> </b>
        </dl>



    </div>
    <div>
       
        
    </div>
    </div>